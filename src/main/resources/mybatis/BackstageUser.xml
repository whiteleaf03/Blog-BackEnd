<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.whiteleaf03.blogbackend.mapper.BackstageUserMapper">

    <!--  用户登录  -->
    <select id="selectSingleBackStageUserByUsername" parameterType="String" resultType="cn.whiteleaf03.blogbackend.entity.BackstageUser">
        select *
        from backstageuser
        where username = #{username};
    </select>

</mapper>